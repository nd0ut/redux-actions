{
  "extends": "airbnb/legacy",
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
    "expect": true
  },
  "rules": {
    "padded-blocks": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-unused-expressions": 0,
    "no-reserved-keys": 0,
    "space-infix-ops": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0
  }
}
